{
    "name": "cinnamon-rs",
    "version": "0.1.2",
    "main": "index.js",
    "types": "index.d.ts",
    "napi": {
        "binaryName": "cinnamon",
        "targets": [
        "aarch64-apple-darwin",
        "x86_64-pc-windows-msvc",
        "x86_64-unknown-linux-gnu"
        ]
    },
    "scripts": {
        "build": "napi build --platform --release",
        "artifacts": "napi artifacts -d ./artifacts"
    },
    "devDependencies": {
        "@napi-rs/cli": "^3.5.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3"
    },
    "engines": {
        "node": ">= 10"
    }
}
